#include "stdio.h"
#include "string.h"
int main()
{
	char strExp[]="3+4";
	
	int l=strExp[0]-'0';
	int r=strExp[2]-'0';
	int v0=l+r;
	printf("v0=%d\n\n",v0);
	
	

	
	char sum[]="1+2+2+1+2+5+4-1-3+4-8";
	int v1=sum[0]-'0';
	
	for(int i=1;i<strlen(sum);i++)
	{
		if(sum[i]=='+')
		{
			int r=sum[i+1]-'0';
			v1=v1+r;
			i++;
		}
		else if(sum[i]=='-')
		{
			int r=sum[i+1]-'0';
			v1=v1-r;
			i++;
		}
	}
	printf("v1=%d\n\n",v1);
	

	
	
	char po[]="2*2/4*1/1*2*3/2";
	int v2=po[0]-'0';
	
	for(i=1;i<strlen(po);i++)
	{
		if(po[i]=='*')
		{
			int r=po[i+1]-'0';
			v2=v2*r;
			i++;
		}
		else if(po[i]=='/')
		{
			int r=po[i+1]-'0';
			v2=v2/r;
			i++;
		}
	}
	printf("v2=%d\n\n",v2);
	

	
	
	char pu[]="2+2*3+2/2-1";
	int v3=2;
	
	for(i=0;i<strlen(pu);i++)
	{
		if(pu[i]=='+')
		{
			if(pu[i+2]=='*')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3+l*r;
			}
			else if(pu[i+2]=='/')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3+l/r;
			}
			else if(pu[i+2]=='\0')
			{
				int r=pu[i+1]-'0';
				v3=v3+r;
			}
		}
		else if(pu[i]=='-')
		{
			if(pu[i+2]=='*')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3-l*r;
			}
			else if(pu[i+2]=='/')
			{
				int l=pu[i+1]-'0';
				int r=pu[i+3]-'0';
				v3=v3-l/r;
			}
			else if(pu[i+2]=='\0')
			{
				int r=pu[i+1]-'0';
				v3=v3-r;
			}
		}
	}
	
	printf("v3=%d\n\n",v3);
	
	
	return 0;
}

